        <child link="camera_side_link"/>
        <parent link="origin"/>
    </joint>

<link name="camera_rear_link">
    <collision>
        <geometry>
            <box size="${camera_link_width} ${camera_link_length} ${camera_link_height}"/>
        </geometry>
    </collision>
    <visual>
    <geometry>
            <box size="${camera_link_width} ${camera_link_length} ${camera_link_height}"/>
        </geometry>
    </visual>
</link>

<joint name="camera_rear_joint" type="fixed">
    <axis xyz="0.0 1.0 0.0"/>
    <origin xyz="${-1*body_length/2} 0.0 ${-body_height/2 + (camera_link_height/2)+(link_width/2)}" rpy="0.0 0.75 0.0"/>
        <child link="camera_rear_link"/>
        <parent link="origin"/>
</joint>



